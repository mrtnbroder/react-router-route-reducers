    "babel": "^6.5.2",
    "eslint-plugin-react": "^4.2.3",
    "eslint": "^2.5.3",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react": "^15.0.0-rc.2",
    "redux": "^3.3.1",
    "rimraf": "^2.5.2"
  }
