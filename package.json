    "reducer",
    "reducers",
    "redux",
    "router",
    "state",
    "stateless"
  ],
  "author": "Martin Broder <hello@martinbroder.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrtnbroder/react-router-route-reducers/issues"
  },
  "homepage": "https://github.com/mrtnbroder/react-router-route-reducers#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-rc.1",
    "react-redux": "^4.0",
    "react-router": "^2.0",
